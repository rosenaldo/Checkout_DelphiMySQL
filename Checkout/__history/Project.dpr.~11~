program Project;

uses
  Vcl.Forms,
  DM.Model in 'src\model\DM.Model.pas' {Model: TDataModule},
  Login in 'src\view\Login\Login.pas' {FrmLogin},
  Menu in 'src\view\Menu\Menu.pas' {FrmMenu},
  Empresa in 'src\view\Cadastros\Empresa.pas' {FRM_EMPRESA},
  Cargos in 'src\view\Cadastros\Cargos.pas' {FrmCargos},
  Fornecedores in 'src\view\Cadastros\Fornecedores.pas' {FrmFornecedores},
  Funcionarios in 'src\view\Cadastros\Funcionarios.pas' {FrmFuncionarios},
  Produtos in 'src\view\Cadastros\Produtos.pas' {FrmProdutos},
  Usuarios in 'src\view\Cadastros\Usuarios.pas' {FrmUsuarios},
  ImprimirBarras in 'src\view\Ferramentas\ImprimirBarras.pas' {FrmImprimirBarras},
  EntradasProdutos in 'src\view\Estoque\EntradasProdutos.pas' {FrmEntradaProdutos},
  EstoqueBaixo in 'src\view\Estoque\EstoqueBaixo.pas' {FrmEstoqueBaixo},
